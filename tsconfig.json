{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"moduleResolution": "NodeNext"
	},
	"include": ["src", "./.eslintrc.cjs", "$lib"]
}
